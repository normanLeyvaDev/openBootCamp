let miAltura = 1.74;

let miAlturaEntero = parseInt(miAltura);
console.log(miAlturaEntero);

let miAlturaFlotante = parseFloat(miAltura);
console.log(miAlturaFlotante);

let miPesoEnKg = 74.5;
let miPesoEnKgFlotante = parseFloat(miPesoEnKg);
console.log(miPesoEnKgFlotante);

let alturaRedondeada = miAltura.toFixed();
console.log(alturaRedondeada);

let miPesoEnKgRedondeado = Math.floor(miPesoEnKg);
console.log(miPesoEnKgRedondeado);

let maxValorJSMasUno = Number.MAX_VALUE + 1;
console.log(maxValorJSMasUno);

let maxValorJS = maxValorJSMasUno;
console.log(maxValorJS);

